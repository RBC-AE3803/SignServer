{
  "AppSettings": {
    "SignService": {
      "Host": "127.0.0.1",
      "Port": 8080,
      "Libs": ["libgnutls.so.30", "./QQApp/libsymbols.so"],
      "Offset": "0x54FBD60",
      "MaxDataLength": 1048576,
      "TimeoutMs": 5000
    },
    "Logging": {
      "Level": "Information",
      "EnableFileLogging": false,
      "LogFilePath": "logs/signserver.log"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "NTQQ_SignServer": "Information"
    }
  },
  "AllowedHosts": "*"
}